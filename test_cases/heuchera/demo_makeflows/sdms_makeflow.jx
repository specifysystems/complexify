{
    "define" : {
        "SPECIES": [
            "Tiarella_polyphylla",
            "Mitella_pauciflora",
            "Heuchera_bracteata",
            "Heuchera_pulchella"
        ],
    },
    "rules" : [
        {
            "command": "create_sdm -n 12 -r \"" + SP + ".rpt\" -z \"" + SP + "_package.zip\" \"species_occurrences/" + SP + ".csv\" environment/ ecoregions.tif ./ \"" + SP + "_model.asc\"",
            "inputs": [
                "\"./species_occurrences/" + SP + ".csv\"",
                "ecoregions.tif",
                "environment/alt.asc",
                "environment/bio1.asc",
                "environment/bio2.asc",
                "environment/bio3.asc",
                "environment/bio4.asc",
                "environment/bio5.asc",
                "environment/bio6.asc",
                "environment/bio7.asc",
                "environment/bio8.asc",
                "environment/bio9.asc",
                "environment/bio10.asc",
                "environment/bio11.asc",
                "environment/bio12.asc",
                "environment/bio13.asc",
                "environment/bio14.asc",
                "environment/bio15.asc",
                "environment/bio16.asc",
                "environment/bio17.asc",
                "environment/bio18.asc",
                "environment/bio19.asc",
            ],
            "outputs": [ "\"" + SP + "_package.zip\"", "\"" + SP + ".rpt\"", "\"" + SP + "_model.asc\"" ]
        } for SP in SPECIES
    ]
}